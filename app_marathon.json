{
  "id": "nodejs-app",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "hanken/nodejs_app:latest",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ]
    }
  },
"labels": {
        "HAPROXY_HTTP": "true",
        "HTTP_PORT_IDX_0_NAME": "test"
    },
  "env": {
    "SERVICE_NAME": "nodejs-app",
    "SERVICE_TAGS": "v1"
  },
  "healthChecks": [
    {
    "path": "/",
    "portIndex": 0,
    "protocol": "HTTP",
    "gracePeriodSeconds": 60,
    "intervalSeconds": 10,
    "timeoutSeconds": 10,
    "maxConsecutiveFailures": 3
    }
  ],
  "cpus": 0.1,
  "mem": 64.0,
  "instances": 2,
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  }
}
